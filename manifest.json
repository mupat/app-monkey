{
  "manifest_version": 2,
  "name": "app monkey",
  "description": "app monkey is alfred for chrome (quick access apps and pages via ALT+A)",
  "version": "1.0.0",
  "options_page": "index.html",
  "browser_action": {
    "default_title": "app monkey",
    "default_popup": "index.html" 
  },
  "permissions": [
    "management",
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+A",
        "mac": "MacCtrl+A"
      },
      "description": "open extension"
    }
  }
}