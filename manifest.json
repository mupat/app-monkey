{
  "manifest_version": 2,
  "name": "app monkey",
  "description": "app monkey is alfred for chrome (quick access apps and pages via CTRL(CMD)+Shift+E)",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "img/icon_256.png",
    "default_title": "app monkey",
    "default_popup": "index.html" 
  },
  "permissions": [
    "management",
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      }
    }
  },
  "icons": { 
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png" 
  }
}